#!/usr/bin/env python
#-*- coding:utf-8 -*-

def compteCar(ca, ch):
    "Renvoie le nombre de fois que l'on rencontre le caractère 'ca' dans la chaîne 'ch'."
    a = 0
    nb_ca = 0
    while a < len(ch):
        if ch[a] == ca:
            nb_ca += 1
        a += 1
    return nb_ca

print compteCar('e', 'Cette phrase est un exemple')
