#!/usr/bin/env python
#-*- coding:utf-8 -*-
from turtle import *

def carre(taille, couleur, angle):
    "fonction qui dessine un carré de taille, de rotation (angle) et de couleur déterminées"
    up()
    color(couleur)
    c = 0
    right(angle)
    down()
    while c < 4:
        forward(taille)
        right(90)
        c = c + 1

def triangle(taille, couleur, angle):
    "fonction qui dessine un triangle équilatéral d'une taille, d'une couleur et d'une orientation déterminées"
    up()
    color(couleur)
    c = 0
    right(angle)
    down()
    while c < 3:
        forward(taille)
        right(120)
        c = c + 1

def etoile5(taille, couleur, angle):
    "fonction qui dessine une étoile à 5 branches d'une taille, d'une couleur et d'une orientation déterminées"
    up()
    color(couleur)
    c = 0
    right(angle)
    down()
    while c < 5:
        forward(taille)
        right(144)
        c = c +1

def etoile6(taille, couleur, angle):
    "fonction qui dessine une étoile à 6 branches en reprenant la fonction triangle"
    triangle(taille, couleur, angle)
    up()
    a = raw_input()
    forward(taille/2)
    a = raw_input()
    left(90)
    a=raw_input()
    forward(taille/4)
    a=raw_input()
    right(360+120-angle)
    a=raw_input()
    down()
    triangle(taille, couleur, 0)

up()
#goto(-500,0)
down()
nb_fig = 0
taille = 50
angle = 180
col_tri = 'blue'
col_carre = 'red'
deplacement = 5
while nb_fig <1:
    etoile6(taille*(nb_fig+1), col_tri, angle)
    up()
    forward(deplacement+taille*(nb_fig+1))
    down()
   # carre(taille*(nb_fig+1), col_carre, angle)
    #up()
   # forward(deplacement+taille*(nb_fig+1))
   # down()

    nb_fig += 1

