#!/usr/bin/env python
#-*- coding:utf-8 -*-

vitesse_saisie = raw_input("Veuillez entrer votre vitesse en miles/heure :")
vitesse_MH = int(vitesse_saisie)

vitesse_KMH = vitesse_MH * 1609 / 1000.
vitesse_MS = vitesse_KMH * 3600 / 1000.

print "Votre vitesse en miles/heure est égale à ",vitesse_KMH, "km/h et", vitesse_MS, "m/s"
