#!/usr/bin/env python
#-*- coding:utf-8 -*-

liste = [32, 5, 12, 8, 3, 75, 2, 15]
list_imp = []
list_pair = []

len_liste = len(liste)

i = 1

while i < len_liste:
    if liste[i] % 2 == 0:
        list_pair.append(liste[i])
    else:
        list_imp.append(liste[i])
    i +=1

print "liste paire : ", list_pair
print "liste impaire : ", list_imp
